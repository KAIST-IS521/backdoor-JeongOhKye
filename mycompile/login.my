setStr r1,"User: "
printf r1
; r100 is buf
gets r100
strcmp r100,"user"
if r255,idok,idfail
: idok
setStr r1,"Password: "
printf r1
gets r100
strcmp r100,"password"
if r255,pwok,pwfail
: pwok
